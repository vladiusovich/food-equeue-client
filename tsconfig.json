{
    "compilerOptions": {
        "target": "ES6",
        "module": "commonjs",
        "lib": [
            "dom",
            "es6"
        ],
        "allowJs": true,
        "jsx": "react-jsx",
        "moduleResolution": "node",
        "isolatedModules": true,
        "noEmit": true,
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": [
        "src"
    ]
}